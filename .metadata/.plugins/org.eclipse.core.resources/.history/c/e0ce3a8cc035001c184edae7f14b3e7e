package com.ektha.bcj.overloading;

public class Manager extends Employee {

	double bonusPercent = 0.2;

	public Manager() {
		// TODO Auto-generated constructor stub
	}
	
	public Manager(int employeeId, String employeeName, double salary) {
		super(employeeId, employeeName, salary);
	}

	public double getSalary(double salary, double bonusPercent) {

		return salary + salary * bonusPercent;
	}

}
